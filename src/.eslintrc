{
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "no-underscore-dangle": [
      "error",
      { "allow": ["_headers", "_id"], "allowAfterThis": true }
    ],
    "no-unused-vars": "off",
    "class-methods-use-this": [
      "error",
      { "exceptMethods": ["_handleResponse", "_getLastWeeksDate"] }
    ]
  }
}
